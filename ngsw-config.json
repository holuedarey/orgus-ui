{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets-core",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/animations/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani|ico)"
        ]
      }
    },
    {
      "name": "assets-others",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "tables",
      "urls": [
        "https://**/users",
        "https://**/clients",
        "https://**/meters"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "5s",
        "maxAge": "7d",
        "maxSize": 100
      },
      "cacheQueryOptions": {
        "ignoreSearch": false
      }
    },
    {
      "name": "drop-down-only",
      "urls": [
        "https://**/roles",
        "https://**/meters/get-meterManufacturers"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "1s",
        "maxAge": "7d",
        "maxSize": 10
      },
      "cacheQueryOptions": {
        "ignoreSearch": true
      }
    }
  ]
}